{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch (AdminService)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/AdminService/bin/Debug/net9.0/AdminService.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/AdminService",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "DOTNET_ENVIRONMENT": "Development"
      },
      "console": "internalConsole"
    }
  ]
}
